{
    "_version": "1.48.0",
    "sap.app": {
        "id": "de.santos.employees",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:basic",
            "version": "1.16.1",
            "toolsId": "af3513a6-1205-451e-9390-2c9062900500"
        },
        "dataSources": {
            "Northwind": {
                "uri": "V2/Northwind/Northwind.svc/",
                "type": "OData",
                "settings": {
                    "annotations": [],
                    "localUri": "localService/Northwind/metadata.xml",
                    "odataVersion": "2.0"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": true,
        "dependencies": {
            "minUI5Version": "1.108.36",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "de.santos.employees.i18n.i18n"
                }
            },
            "northwindModel": {
                "preload": true,
                "dataSource": "Northwind",
                "settings": {
                    "defaultBindingMode": "TwoWay",
                    "defaultCountMode": "Inline",
                    "refreshAfterChange": false
                }
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "de.santos.employees.view",
                "controlAggregation": "pages",
                "controlId": "app",
                "clearControlAggregation": false
            },
            "routes": [
                {
                    "name": "RouteMaster",
                    "pattern": ":?query:",
                    "target": [
                        "TargetMaster"
                    ]
                },
                {
                    "name": "RouteDetails",
                    "pattern": "Employees({index})",
                    "target": [
                        "TargetDetails"
                    ]
                }
            ],
            "targets": {
                "TargetContainer": {
                    "viewType": "XML",
                    "transition": "flip",
                    "clearControlAggregation": false,
                    "viewId": "Container",
                    "viewName": "Container"
                },
                "TargetMaster": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "Master",
                    "viewName": "Master",
                    "parent": "TargetContainer",
                    "controlId": "idFlexibleColumnLayout",
                    "controlAggregation": "beginColumnPages"
                },
                "TargetDetails": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "Details",
                    "viewName": "Details",
                    "parent": "TargetContainer",
                    "controlId": "idFlexibleColumnLayout",
                    "controlAggregation": "midColumnPages"
                }
            }
        },
        "rootView": {
            "viewName": "de.santos.employees.view.App",
            "type": "XML",
            "async": true,
            "id": "App"
        }
    }
}